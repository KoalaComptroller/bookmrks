<%= form_for(Bookmark.new, url: bookmarks_path) do |f| %>
	<div class="form-group">
		<%= f.label :title %>
		<%= f.text_field :title, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= f.label :url %>
		<%= f.text_field :url, class: "form-control" %>
	</div>

	<%= f.submit "Add Bookmark", class: "btn btn-primary" %>
<% end %>