<div class="row">
<div class="col-sm-4 well">
	<h5>My Bookmarks</h5>
	<em>Placeholder for folders</em>
</div>

<div class="col-sm-8">
	<% if @user.bookmarks.any? %>
		<h4>Bookmarks (<%= @user.bookmarks.count %>)</h4>
		<ul class="bookmarks">
			<%= render @bookmarks %>
		</ul>
		<%= will_paginate @bookmarks %>
	<% end %>
</div>
</div>